@model eAdministrationLabs.Dtos.Create.CreateEquiLabDto

@{
    ViewData["Title"] = "Create";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}



<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group" >
                <label asp-for="EquipmentId" class="control-label"></label>
                <select asp-for="EquipmentId" class="form-control" asp-items="ViewBag.EquipmentId" style="border: 1px solid #ccc; border-radius: 6px;"></select>
            </div>
            <div class="form-group">
                <label asp-for="LabId" class="control-label"></label>
                <select asp-for="LabId" class="form-control" asp-items="ViewBag.LabId" style="border: 1px solid #ccc; border-radius: 6px;"></select>
            </div>

            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

